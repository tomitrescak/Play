{
  "variables" : [
    {
      "name" : "modelName",
      "description" : "What's your model called (Meteor.Collection)?"
    }
  ],
  "files" : [
    {
      "template" : "model.next.js",
      "path" : "model/__modelName__.next.js"
    },
    {
      "template" : "subscription.next.js",
      "path" : "client/subscriptions/__modelName__Sub.next.js"
    },
    {
      "template" : "publication.next.js",
      "path" : "server/publications/__modelName__Pub.next.js"
    }
  ]
}
